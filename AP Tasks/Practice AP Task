import random
print("Welcome To The Game High Or Low\n The Object Is To Guess Correctly 10 Times To Win The Game")
num1 = random.randint(1,15)
print(num1)
user_input = input("High Or Low? ")

while user_input == 'High':
    num2 = random.randint(1,15)
    print(num2)
    if num2 > num1:
        print("Good Job")
        user_input = input("High Or Low? ")
    elif num2 < num1:
        print("Sorry, Better Luck Next Time")
        break
    elif num2 == num1:
        print("It's The Same, Retry")
        num2 = random.randint(1,15)
        print(num2)
        user_input = input("High Or Low? ")
while user_input == 'Low':
    num2 = random.randint(1,15)
    print(num2)
    if num2 < num1:
        print("Good Job")
        user_input = input("High Or Low? ")
    elif num2 > num1:
        print("Sorry, Better Luck Next Time")
        break
    elif num2 == num1:
        print("It's The Same, Retry")
        num2 = random.randint(1,15)
        print(num2)
        user_input = input("High Or Low? ")
        
    
    
        